[Variables]
%ProgramTitle%=qEmu-X
%ProgramEXE%=qemu-system-i386.exe
%ProgramFolder%=qEmuX
%WF%=%ProjectTemp%\%ProgramFolder%
%hddInclude%=Nop
%DiskBoot%=0
%FileContainer%=%FileContainer_x86%
%FileContainer_x64_Project%=%ScriptDir%\qEmu_X_x64_File.Script
%FileContainer_x64%=%BaseDir%\Projects\Yomi\VirtualTestY\qEmu_X_x64_File.Script
%FileContainer_x86%=%BaseDir%\Projects\Yomi\VirtualTestY\qEmu_X_x86_File.Script
%FileContainer_NT5_x86%=%BaseDir%\Projects\Yomi\VirtualTestY\qEmu_X_NT5x_x86_File.Script
%FileContainer_NT5_x64%=%BaseDir%\Projects\Yomi\VirtualTestY\qEmu_X_NT5x_x64_File.Script

[Process]
Call,HostOSVersion,,1
Call,HostOSArch
//2016-10-16: New QEMU installers. Drop SDL and Windows XP support, use updated third party DLLs, support more QEMU features.
//If,%HostOSVersion1%,Smaller,6,Exit,"%ScriptTitle% only support Vista and later"
//--
//---- FileContainer Check - Start
If,%HostOSVersion1%,Equal,5,Begin
If,%HostOSArch%,Equal,x86,Begin
Set,%FileContainer%,%FileContainer_NT5_x86%
If,Not,ExistFile,%FileContainer%,Run,%ScriptFile%,DownloadXXX_Container_NT5x_x86_NOQ
End
//-
If,%HostOSArch%,Equal,x64,Begin
Set,%FileContainer%,%FileContainer_NT5_x64%
If,Not,ExistFile,%FileContainer%,Run,%ScriptFile%,DownloadXXX_Container_NT5x_x64_NOQ
If,Not,ExistFile,%FileContainer%,Begin
//x64 Fall Back to x86
Run,%ScriptFile%,DownloadXXX_Container_NT5x_x86_NOQ
If,ExistFile,%FileContainer_NT5_x86%,Set,%FileContainer%,%FileContainer_NT5_x86%
End
End
//-
// All FallBack to x86
If,Not,%HostOSArch%,Equal,x86,Begin
If,Not,%HostOSArch%,Equal,x64,Begin
Set,%FileContainer%,%FileContainer_NT5_x86%
If,Not,ExistFile,%FileContainer%,Run,%ScriptFile%,DownloadXXX_Container_NT5x_x86_NOQ
End
End
End
//---------------------------------
If,%HostOSVersion1%,Bigger,5,Begin
If,%HostOSArch%,Equal,x86,Begin
Set,%FileContainer%,%FileContainer_x86%
If,Not,ExistFile,%FileContainer%,Run,%ScriptFile%,DownloadXXX_Container_x86_NOQ
End
//-
If,%HostOSArch%,Equal,x64,Begin
//-
//SE project have FileContainer_x64 on project distribution - Gena do not
Set,%FileContainer%,%FileContainer_x64_Project%
If,Not,ExistFile,%FileContainer%,Set,%FileContainer%,%FileContainer_x64%
If,Not,ExistFile,%FileContainer%,Run,%ScriptFile%,DownloadXXX_Container_x64_NOQ
If,Not,ExistFile,%FileContainer%,Begin
//x64 Fall Back to x86
Run,%ScriptFile%,DownloadXXX_Container_x86_NOQ
If,ExistFile,%FileContainer_x86%,Set,%FileContainer%,%FileContainer_x86%
End
End
//-
// All FallBack to x86
If,Not,%HostOSArch%,Equal,x86,Begin
If,Not,%HostOSArch%,Equal,x64,Begin
Set,%FileContainer%,%FileContainer_x86%
If,Not,ExistFile,%FileContainer%,Run,%ScriptFile%,DownloadXXX_Container_x86_NOQ
End
End
End
//-
//---- FileContainer Check - Finish
//--
//Debug ----- S
If,%SelectContainer_ScrollBox%,Equal,"Force qEmu x86",Set,%FileContainer%,%FileContainer_x86%
If,%SelectContainer_ScrollBox%,Equal,"Force qEmu x64",Set,%FileContainer%,%FileContainer_x64%
If,%SelectContainer_ScrollBox%,Equal,"Force qEmu x64 Project",Set,%FileContainer%,%FileContainer_x64_Project%
If,%SelectContainer_ScrollBox%,Equal,"Force qEmu NT5 x64",Set,%FileContainer%,%FileContainer_NT5_x64%
If,%SelectContainer_ScrollBox%,Equal,"Force qEmu NT5 x86",Set,%FileContainer%,%FileContainer_NT5_x86%
//--
//Set,%FileContainer%,%FileContainer_x86%
//Set,%FileContainer%,%FileContainer_x64%
//Set,%FileContainer%,%FileContainer_x64_Project%
//Set,%FileContainer%,%FileContainer_NT5_x86%
//Set,%FileContainer%,%FileContainer_NT5_x64%
//Debug ----- F
//--
If,Not,ExistFile,%FileContainer%,Call,Echo,"%FileContainer% NOT FOUND",Warn,,Message,,Exit
//--
If,%FileContainer%,Equal,%FileContainer_x64%,Set,%WF%,%WF%_x64
If,%FileContainer%,Equal,%FileContainer_x64_Project%,Set,%WF%,%WF%_x64
If,%FileContainer%,Equal,%FileContainer_x86%,Set,%WF%,%WF%_x86
If,%FileContainer%,Equal,%FileContainer_NT5_x64%,Set,%WF%,%WF%_NT5_x64
If,%FileContainer%,Equal,%FileContainer_NT5_x86%,Set,%WF%,%WF%_NT5_x86
//--
Echo,"Processing %ScriptTitle%..."
//--
If,Not,ExistFile,%WF%\%ProgramEXE%,Run,%ScriptFile%,Extract
//--
If,%VirtualOS_ScrollBox%,Equal,Auto,Begin
If,%SourceArch%,Equal,x86,Set,%Exe%,qemu-system-i386.exe
If,%SourceArch%,Equal,x64,Set,%Exe%,qemu-system-x86_64.exe
End
Else,Set,%Exe%,%VirtualOS_ScrollBox%
Echo,%Exe%
//-
Set,%BootDrive_ScrollBoxV%,%BootDrive_ScrollBox%
//--
If,Not,ExistVar,%ISOfileVar%,Begin
Set,%ISOfileVar%,%ISOfile%
Set,%t%,""
IniRead,%ProjectDir%\script.project,IniVariables,ISOfileVar,%t%
If,Not,%t%,Equal,"",Set,%ISOfileVar%,%t%
End
//-
Set,%Ram%,%Ram_NumberBox%
//-
Set,%Ex%,Ex
If,%Pause_CheckBox%,Equal,True,Set,%Ex%,""
//-
If,%Hdd_Include_CheckBox%,Equal,True,Set,%hddInclude%,Yep
//-
Set,%hdDiskImage1%,""
If,%hddInclude%,Equal,Yep,Begin
Set,%hdDiskImage1%,-hda#$s#$q%WF%\%Hdd_Disk_Image%#$q
If,Not,ExistFile,%WF%\%Hdd_Disk_Image%,Begin
ExtractFile,%ScriptFile%,Folder,Template_Fat32.img,%WF%
FileRename,%WF%\Template_Fat32.img,%WF%\%Hdd_Disk_Image%
End
End
//-
Set,%PhysicalDriveA%,""
If,%Include_PhysicalDrive_CheckBox%,Equal,True,Begin
If,%DiskBoot%,Equal,1,Begin
If,Not,%Select_Disk_ScrollBox%,Equal,%Include_PhysicalDrive_ScrollBox%,Set,%hdbE%,1
End
Else,Set,%hdbE%,1
//-
If,%hdbE%,Equal,1,Set,%PhysicalDriveA%," -hdb //./%Include_PhysicalDrive_ScrollBox%"
End
//-
If,%Hdd_Disk_Reset_CheckBox%,Equal,True,Begin
If,ExistFile,%WF%\%Hdd_Disk_Image%,FileDelete,%WF%\%Hdd_Disk_Image%
ExtractFile,%ScriptFile%,Folder,Template_Fat32.img,%WF%
FileRename,%WF%\Template_Fat32.img,%WF%\%Hdd_Disk_Image%
End
//-
//Auto Add Space to Additional Parameters
Set,%Pars_TextBoxV%,%Pars_TextBox%
If,Not,%Pars_TextBoxV%-,Equal,-,Begin
StrFormat,LEFT,%Pars_TextBoxV%,1,%t%
If,%t%,Equal," ",Echo,"Everything OK"
Else,Set,%Pars_TextBoxV%," %Pars_TextBoxV%"
End
//-
Set,%Par_LocalTime%,""
If,%Par_LocTi_CheckBox%,Equal,True,Set,%Par_LocalTime%,#$s-localtime
Set,%Par_FullScreen%,""
If,%Par_FScrn_CheckBox%,Equal,True,Set,%Par_FullScreen%,#$s-full-screen
Set,%Par_Usb%,""
If,%Par_EnUSB_CheckBox%,Equal,True,Set,%Par_Usb%,#$s-usb
Set,%Par_NO_acpi%,""
If,%Par_dACPI_CheckBox%,Equal,True,Set,%Par_NO_acpi%,#$s-no-acpi
Set,%Par_USB_Dev_tablet%,""
//-usbdevice tablet depricated -- see manual
//If,%Par_USBD_tablet_CheckBox%,Equal,True,Set,%Par_USB_Dev_tablet%,#$s-usbdevice#$stablet
If,%Par_USBD_tablet_CheckBox%,Equal,True,Set,%Par_USB_Dev_tablet%,#$s-device#$susb-tablet
//You muse enable USB
If,%Par_USBD_tablet_CheckBox%,Equal,True,Set,%Par_Usb%,#$s-usb
If,%Par_USBD_tablet_CheckBox%,Equal,True,Begin
If,Not,%Par_EnUSB_CheckBox%,Equal,True,Begin
Set,%Par_EnUSB_CheckBox%,True,Permanent
System,RefreshInterface
End
End
//-
Set,%w%,%WF%\1Debug_RunAsAdmin.cmd
If,ExistFile,%w%,Shellexecute,Hide,cmd.exe,"/C del /q /f #$q%w%#$q"
If,ExistFile,%w%,Echo,"FILE NOT DELETED",WARN
FileCreateBlank,%w%
If,%DiskBoot%,Equal,1,TxtAddLine,%w%,"Echo DiskBoot",Append
If,Not,%DiskBoot%,Equal,1,TxtAddLine,%w%,"Echo ISO Boot",Append
TxtAddLine,%w%,"Echo Some features of qEmu ex: -usb requires RunAsAdmin",Append
TxtAddLine,%w%,"cd /d #$q#$p~dp0#$q",Append
TxtAddLine,%w%,"Set ISOfileVar=%ISOfileVar%",Append
TxtAddLine,%w%,"Del /q /f 1Debug_ErrorExists.txt",Append
//-
If,Not,%DiskBoot%,Equal,1,Begin
TxtAddLine,%w%,"%Exe% %hdDiskImage1%%PhysicalDriveA%#$s-boot %BootDrive_ScrollBoxV% -cdrom #$q%ISOfileVar%#$q -L #$q%WF%#$q -net nic,model=rtl8139 -net user -m %Ram%%Par_LocalTime%%Par_FullScreen%%Par_Usb%%Par_NO_acpi%%Par_USB_Dev_tablet%%Pars_TextBoxV%",Append
Echo,"%Exe% %hdDiskImage1%%PhysicalDriveA%#$s-boot %BootDrive_ScrollBoxV% -cdrom #$q%ISOfileVar%#$q -L #$q%WF%#$q -net nic,model=rtl8139 -net user -m %Ram%%Par_LocalTime%%Par_FullScreen%%Par_Usb%%Par_NO_acpi%%Par_USB_Dev_tablet%%Pars_TextBoxV%"
End
If,%DiskBoot%,Equal,1,Begin
Set,%BootDrive_ScrollBoxV%,c
StrFormat,REPLACE,%hdDiskImage1%,-hda,-hdb,%hdDiskImage1%
//TxtAddLine,%w%,"%Exe% -hda //./%Select_Disk_ScrollBox% %hdDiskImage1%%PhysicalDriveA%#$s-boot %BootDrive_ScrollBoxV% -cdrom #$q%ISOfileVar%#$q -L #$q%WF%#$q -net nic,model=rtl8139 -net user -m %Ram%%Par_LocalTime%%Par_FullScreen%%Par_Usb%%Par_NO_acpi%%Par_USB_Dev_tablet%%Pars_TextBoxV%",Append
TxtAddLine,%w%,"%Exe% -hda \\.\%Select_Disk_ScrollBox% %hdDiskImage1%%PhysicalDriveA%#$s-boot %BootDrive_ScrollBoxV% -L #$q%WF%#$q -net nic,model=rtl8139 -net user -m %Ram%%Par_LocalTime%%Par_FullScreen%%Par_Usb%%Par_NO_acpi%%Par_USB_Dev_tablet%%Pars_TextBoxV%",Append
Echo,"%Exe% -hda \\.\%Select_Disk_ScrollBox% %hdDiskImage1%%PhysicalDriveA%#$s-boot %BootDrive_ScrollBoxV% -L #$q%WF%#$q -net nic,model=rtl8139 -net user -m %Ram%%Par_LocalTime%%Par_FullScreen%%Par_Usb%%Par_NO_acpi%%Par_USB_Dev_tablet%%Pars_TextBoxV%"
End
//-
TxtAddLine,%w%,"Echo #$pErrorLevel#$p",Append
TxtAddLine,%w%,"::pause",Append
TxtAddLine,%w%,"If #$pErrorLevel#$p NEQ 0 Goto :ErrorExists",Append
TxtAddLine,%w%,"::pause",Append
TxtAddLine,%w%,"exit",Append
TxtAddLine,%w%,"",Append
