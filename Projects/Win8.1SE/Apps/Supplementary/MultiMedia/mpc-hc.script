[Variables]
%ProgramTitle%=MPlayer
%ProgramEXE%=mpc-hc.exe
%ProgramEXEx64%=mpc-hc64.exe
%ProgramFolder%=Mpc-hc
%LAVFilters%=LAVFilters
%ContainerFileFolder%=%scriptdir%
%ContainerFileName%=mpc-hc_file.script
%ContainerFile%=%ContainerFileFolder%\%ContainerFileName%
%SetupFile%=MPC-HC.1.7.13.x86.7z
%SetupFilex64%=MPC-HC.1.7.13.x64.7z

[Process]
If,Not,ExistFile,%ContainerFile%,Exit,"'%ScriptTitle%' Container File Missing %ContainerFile%"
//--
Echo,"Processing %ScriptTitle%..."
IniRead,%ProjectDir%\script.project,IniVariables,NoWarnRegOverWrite,%NoWarnROW%
//-
StrFormat,REPLACE,%ScrollBox_RunFromWhere%,"Run from ","",%Here%
If,Not,%Here%,Equal,Normal,RunFrom,%Here%
//--
If,%SourceArch%,Equal,x64,Begin
Set,%ProgramFolder%,%ProgramFolder%_x64
Set,%SetupFile%,%SetupFilex64%
Set,%ProgramEXE%,%ProgramEXEx64%
Set,%LAVFilters%,%LAVFilters%64
End
StrFormat,RTRIM,%SetupFile%,3,%SetupFolder%
Run,%ScriptFile%,Extract
//--
If,Not,ExistDir,%Target_Prog%,DirMake,%Target_Prog%
If,ExistDir,%Target_Prog%\%ProgramFolder%,Shellexecute,Hide,cmd.exe,"/C rd /s /q #$q%Target_Prog%\%ProgramFolder%#$q"
If,ExistDir,%Target_Prog%\%ProgramFolder%,Call,DirDelete,%Target_Prog%\%ProgramFolder%
If,Not,ExistDir,%Target_Prog%\%ProgramFolder%\Lang,DirMake,%Target_Prog%\%ProgramFolder%\Lang
FileCopy,%ProjectTemp%\TempExtractFolder\%ProgramFolder%\%SetupFolder%\*,%Target_Prog%\%ProgramFolder%,NoRec
//--
// Language for resource language - LangID is used for Interface Language in registry
StrFormat,SPLIT,%ScrollBox_Lang%,|,2,%LangID%
StrFormat,SPLIT,%ScrollBox_Lang%,|,3,%Lang%
If,%Lang%,Equal,al,Begin
FileCopy,%ProjectTemp%\TempExtractFolder\%ProgramFolder%\%SetupFolder%\Lang\*,%Target_Prog%\%ProgramFolder%\Lang
End
Else,Begin
// for English (default), mpc-hc needs at least 1 language resource (french language copied)
If,%Lang%,Equal,en,FileCopy,%ProjectTemp%\TempExtractFolder\%ProgramFolder%\%SetupFolder%\Lang\mpcresources.fr.dll,%Target_Prog%\%ProgramFolder%\Lang
Else,Begin
If,ExistFile,%ProjectTemp%\TempExtractFolder\%ProgramFolder%\%SetupFolder%\Lang\mpcresources.%Lang%.dll,FileCopy,%ProjectTemp%\TempExtractFolder\%ProgramFolder%\%SetupFolder%\Lang\mpcresources.%Lang%.dll,%Target_Prog%\%ProgramFolder%\Lang
Else,Begin
// for English (default), mpc-hc needs at least 1 language resource (french language copied)
FileCopy,%ProjectTemp%\TempExtractFolder\%ProgramFolder%\%SetupFolder%\Lang\mpcresources.fr.dll,%Target_Prog%\%ProgramFolder%\Lang
Call,Echo,"%ProjectTemp%\TempExtractFolder\%ProgramFolder%\%SetupFolder%\Lang\mpcresources.%Lang%.dll NOT FOUND",Warn
End
End
End
//--
// Dependencies
If,Not,%OSFamily%,Equal,NT5,Run,%ScriptFile%,Dependencies_mpchc
Else,Run,%ScriptFile%,Dependencies_mpchc_NT5
// ShortCuts
Run,%ScriptFile%,ShortCuts
//--
If,%TextBox_Playlist%-,Equal,-,Set,%TextBox_Playlist%,"Add to %ProgramTitle% Playlist"
If,%TextBox_Play%-,Equal,-,Set,%TextBox_Play%,"Play with %ProgramTitle%"
//--
// Registry
RegHiveLoad,Tmp_Default,%RegDefault%
RegHiveLoad,Tmp_Software,%RegSoftware%
Run,%ScriptFile%,AddRegMPC
Echo,"Processing %ScriptTitle% Associations..."
If,%CheckBox_AllFilesAsso%,Equal,True,Run,%ScriptFile%,AddRegAllFiles
//If,%CheckBox_DirAsso%,Equal,True,Run,%ScriptFile%,AddRegDir
If,%ScrollBox_Asso%,Equal,"WMP Associations",Begin
If,%CheckBox_AudioAsso%,Equal,True,Run,%ScriptFile%,AddRegAudioWMP
If,%CheckBox_VideoAsso%,Equal,True,Run,%ScriptFile%,AddRegVideoWMP
End
Else,Begin
If,%CheckBox_AudioAsso%,Equal,True,Run,%ScriptFile%,AddRegAudioMPC
If,%CheckBox_VideoAsso%,Equal,True,Run,%ScriptFile%,AddRegVideoMPC
End
Run,%ScriptFile%,AddRegMidi
RegHiveUnLoad,Tmp_Software
RegHiveUnLoad,Tmp_Default

[Extract]
//Extracting-7z-Start
If,ExistDir,%ProjectTemp%\TempExtractFolder\%ProgramFolder%,Shellexecute,Hide,cmd.exe,"/C rd /s /q #$q%ProjectTemp%\TempExtractFolder\%ProgramFolder%#$q"
If,ExistDir,%ProjectTemp%\TempExtractFolder\%ProgramFolder%,Call,DirDelete,%ProjectTemp%\TempExtractFolder\%ProgramFolder%
DirMake,%ProjectTemp%\TempExtractFolder\%ProgramFolder%
ExtractFile,%ContainerFile%,Folder,%SetupFile%,%ProjectTemp%\TempExtractFolder\%ProgramFolder%
ShellExecute,Hide,%Tools%\7z.exe,"x #$q%ProjectTemp%\TempExtractFolder\%ProgramFolder%\%SetupFile%#$q -y -o#$q%ProjectTemp%\TempExtractFolder\%ProgramFolder%#$q"
//Extracting-7z-Finish
If,%CheckBox_LAV%,Equal,True,If,ExistDir,%ProjectTemp%\TempExtractFolder\%ProgramFolder%\%SetupFolder%\%LAVFilters%,Call,DirDelete,%ProjectTemp%\TempExtractFolder\%ProgramFolder%\%SetupFolder%\%LAVFilters%
If,ExistDir,%ProjectTemp%\TempExtractFolder\%ProgramFolder%\%SetupFolder%\Shaders,Call,DirDelete,%ProjectTemp%\TempExtractFolder\%ProgramFolder%\%SetupFolder%\Shaders
If,ExistDir,%ProjectTemp%\TempExtractFolder\%ProgramFolder%\%SetupFolder%\CrashReporter,Call,DirDelete,%ProjectTemp%\TempExtractFolder\%ProgramFolder%\%SetupFolder%\CrashReporter

[Launch_Program]
Call,HostOSArch
Run,%ScriptFile%,Extract
If,%HostOSArch%,Equal,x64,Begin
Set,%SetupFile%,%SetupFilex64%
Set,%ProgramEXE%,%ProgramEXEx64%
Set,%LAVFilters%,%LAVFilters%64
End
StrFormat,RTRIM,%SetupFile%,3,%SetupFolder%
Run,%ScriptFile%,Extract
//-
Call,Start,,%ProgramEXE%,,%ProjectTemp%\TempExtractFolder\%ProgramFolder%\%SetupFolder%

