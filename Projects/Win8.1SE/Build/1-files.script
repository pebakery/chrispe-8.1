[PInstallWimUpdated]
Set,%InstallWimUpdated%,Noo
Retrieve,FILEVERSION,%b32%\hal.dll,%halBootVersion%
Retrieve,FILEVERSION,%i32%\hal.dll,%halInstallVersion%
If,Not,%halBootVersion%,Equal,%halInstallVersion%,Set,%InstallWimUpdated%,Yes
Else,Begin
Retrieve,FILEVERSION,%b32%\UXInit.dll,%UXInitBootVersion%
Retrieve,FILEVERSION,%i32%\UXInit.dll,%UXInitInstallVersion%
If,Not,%UXInitBootVersion%,Equal,%UXInitInstallVersion%,Set,%InstallWimUpdated%,Yes
Else,Begin
Retrieve,FILEVERSION,%iw%\explorer.exe,%explorerInstallVersion%
If,Not,%explorerInstallVersion%,Equal,%halBootVersion%,Set,%InstallWimUpdated%,Yes
End
End
// if Install.wim updated, instead of copying boot.wim folders, create a list of boot.wim files. Then copy these files from Install.wim, if exist, otherwise copy from boot.wim
If,%InstallWimUpdated%,Equal,Yes,Begin
If,ExistDir,%ProjectTemp%\ListFilesPlugin,DirDelete,%ProjectTemp%\ListFilesPlugin
If,ExistDir,%ProjectTemp%\ListFilesPlugin,Shellexecute,Hide,%CmdexeFolder%cmd.exe,"/C rd /s /q #$q%ProjectTemp%\ListFilesPlugin#$q"
If,ExistDir,%ProjectTemp%\ListFilesPlugin,Call,DirDelete,%ProjectTemp%\ListFilesPlugin
If,ExistDir,%ProjectTemp%\ListFilesPlugin,Call,Echo,"%ProjectTemp%\ListFilesPlugin could not be deleted!",Warn
If,Not,ExistDir,%ProjectTemp%\ListFilesPlugin,DirMake,%ProjectTemp%\ListFilesPlugin
End

