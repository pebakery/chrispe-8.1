[Grub4Dos_Menu]
// Grub4Dos Bootmanager entries
If,Not,ExistFile,%ProjectTemp%\TempExtractFolder\%ProgramFolder%\hexalter.exe,Begin
ExtractFile,%ScriptFile%,Folder,hexalter.exe,%ProjectTemp%\TempExtractFolder\%ProgramFolder%
End
IniRead,%ProjectDir%\script.project,IniVariables,OS,%OS%
IniRead,%ProjectDir%\script.project,IniVariables,OSServicePack,%OSServicePack%
If,Not,%OS%,Equal,Win7,Run,%ScriptFile%,ErrorMsg,"'%ScriptTitle%' Only for Windows 7 SP0/SP1 sources"
If,Not,%OSServicePack%,Equal,SP0,If,Not,%OSServicePack%,Equal,SP1,Run,%ScriptFile%,ErrorMsg,"'%ScriptTitle%' Only for Windows 7 SP0/SP1 sources"
//-
If,%pCheckBox4%,Equal,True,Run,%ScriptFile%,GRUB_WIM,4
If,%pCheckBox3%,Equal,True,Run,%ScriptFile%,GRUB_WIM,3
If,%pCheckBox2%,Equal,True,Run,%ScriptFile%,GRUB_WIM,2

[GRUB_WIM]
If,Not,ExistFile,%pFileBox#1%,Run,%ScriptFile%,Warn_Message,"WimFile #1 selected does not exist. It will not be added"
Else,Begin
strformat,filename,%pFileBox#1%,%WIM#1_Filename%
Echo,"Copy %WIM#1_Filename%..."
FileCopy,%pFileBox#1%,%OtherOS_Target%
//
FileCopy,%SourceDir%\bootmgr,%TargetDir%\BOOTMGR#1
