[Addboot3264]
If,Not,ExistFile,%TargetDir%\efi\microsoft\boot\bcd,Echo,"No EFI Boot Configuration Data (BCD), bootia32.efi or bootx64.efi will not be added.",Warn
Else,Begin
If,%SourceArch%,Equal,x64,Begin
Echo,"Copy bootia32.efi..."
If,ExistFile,#1\bootia32.efi,FileCopy,#1\bootia32.efi,%TargetDir%\efi\boot
Else,Run,%ScriptFile%,Warn_Message,"#1\bootia32.efi does not exist. It will not be added"
// optional, copy memtest.efi previously renamed in memtestx64.efi
If,ExistFile,#1\memtestx86.efi,Begin
Echo,"Copy memtestx86.efi and Add BCD entry..."
FileCopy,#1\memtestx86.efi,%TargetDir%\efi\microsoft\boot
run,%ScriptFile%,AddmemtestBCD
End
End
If,%SourceArch%,Equal,x86,Begin
Echo,"Copy bootx64.efi..."
If,ExistFile,#1\bootx64.efi,FileCopy,#1\bootx64.efi,%TargetDir%\efi\boot
Else,Run,%ScriptFile%,Warn_Message,"#1\bootx64.efi does not exist. It will not be added"
// optional, copy memtest.efi previously renamed in memtestx64.efi
If,ExistFile,#1\memtestx64.efi,Begin
Echo,"Copy memtestx64.efi and Add BCD entry..."
FileCopy,#1\memtestx64.efi,%TargetDir%\efi\microsoft\boot
run,%ScriptFile%,AddmemtestBCD
End
End
End
// Make Image with bootia32.efi and bootx64.efi for ISO creation
If,ExistFile,%TargetDir%\efi\boot\bootia32.efi,If,ExistFile,%TargetDir%\efi\boot\bootx64.efi,run,%ScriptFile%,Make3264image

[Make3264image]
// Check/Install IMDISK
Set,%ImDiskInstalled%,True
Call,HostSystemDir
System,FILEREDIRECT,OFF
If,Not,ExistFile,%HostSystemDir%\imdisk.exe,Set,%ImDiskInstalled%,False
System,FILEREDIRECT,ON
If,%ImDiskInstalled%,Equal,False,Begin
If,Not,ExistFile,%ProjectDir%\Utils\Imdisk_Installer.script,Message,"Imdisk_Installer Plugin required to install ImDisk and create the 32/64 bit efisys.img image!!!",Error
Else,Begin
Echo,"Install Imdisk Unattended..."
If,Not,ExistDir,%ProjectTemp%,DirMake,%ProjectTemp%
ExtractFile,%ProjectDir%\Utils\Imdisk_Installer.script,files,imdiskinst.exe,%ProjectTemp%
ShellExecuteDelete,hide,%ProjectTemp%\imdiskinst.exe,-y
End
End
//-
Echo,"Create the 32/64 bit efisys.img image and replace original efisys*.bin..."
If,ExistDir,%ProjectTemp%\Make3264img,DirDelete,%ProjectTemp%\Make3264img
If,ExistDir,%ProjectTemp%\Make3264img,Shellexecute,Hide,cmd.exe,"/C rd /s /q #$q%ProjectTemp%\Make3264img#$q"
If,ExistDir,%ProjectTemp%\Make3264img,Echo,"%ProjectTemp%\Make3264img folder NOT DELETED!!!",Warn
If,Not,ExistDir,%ProjectTemp%\Make3264img,DirMake,%ProjectTemp%\Make3264img
//- Extract Grub2
ExtractFile,%ScriptFile%,Folder,Make3264img.cmd,%ProjectTemp%\Make3264img
ShellExecute,Open,%ProjectTemp%\Make3264img\Make3264img.cmd,"#$q%TargetDir%\efi\boot#$q #$q%ProjectTemp%\Make3264img#$q efisys.img",%ProjectTemp%\Make3264img
If,ExistFile,%ProjectTemp%\Make3264img\efisys.img,Begin
If,ExistFile,%TargetDir%\efi\microsoft\boot\efisys.bin,Shellexecute,Hide,cmd.exe,"/C del /q /f #$q%TargetDir%\efi\microsoft\boot\efisys.bin#$q"
If,ExistFile,%TargetDir%\efi\microsoft\boot\efisys.bin,Call,FileDelete,%TargetDir%\efi\microsoft\boot\efisys.bin
If,ExistFile,%TargetDir%\efi\microsoft\boot\efisys.bin,Call,Echo,"TargetDir\efi\microsoft\boot\efisys.bin NOT DELETED and not replaced!!! ISO will not be compatible 32/64 bit UEFI",Warn
Else,FileCopy,%ProjectTemp%\Make3264img\efisys.img,%TargetDir%\efi\microsoft\boot\efisys.bin
//-
If,ExistFile,%TargetDir%\efi\microsoft\boot\efisys_noprompt.bin,Shellexecute,Hide,cmd.exe,"/C del /q /f #$q%TargetDir%\efi\microsoft\boot\efisys_noprompt.bin#$q"
If,ExistFile,%TargetDir%\efi\microsoft\boot\efisys_noprompt.bin,Call,FileDelete,%TargetDir%\efi\microsoft\boot\efisys_noprompt.bin
If,ExistFile,%TargetDir%\efi\microsoft\boot\efisys_noprompt.bin,Call,Echo,"TargetDir\efi\microsoft\boot\efisys_noprompt.bin NOT DELETED and not replaced!!! ISO will not be compatible 32/64 bit UEFI",Warn
Else,FileCopy,%ProjectTemp%\Make3264img\efisys.img,%TargetDir%\efi\microsoft\boot\efisys_noprompt.bin
End
Else,Call,Echo,"32/64 bit efisys.img image NOT CREATED!!! ISO will not be compatible with 32/64 bit UEFI",Warn

