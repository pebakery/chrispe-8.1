[Process]
Echo,"Processing Multiboot-WIM..."
//ReadENV
If,Not,ExistVar,%BootSector%,If,ExistSection,%projectdir%\script.project,SetOtherOS,Run,%projectdir%\script.project,SetOtherOS
If,%BcdGrub_ScrollBox%,Equal,Grub4Dos-Menu,Begin
If,Not,%BootSector%,Equal,Grub4DOS,Run,%scriptfile%,Warn_Echo,"'%ScriptTitle%' Grub4dos menu selected incompatible with %BootSector% boot manager"
If,Not,%BootSector%,Equal,Grub4DOS,Exit,"'%ScriptTitle%' Grub4dos menu selected incompatible with %BootSector% boot manager"
End
//-
If,Not,ExistFile,%Tools%\%SourceArch%\bcdedit.exe,FileCopy,%InstallSRC%\Windows\System32\bcdedit.exe,%Tools%\%SourceArch%
Set,%BcdEditExe%,%Tools%\%OSArch%\bcdedit.exe
If,Not,ExistFile,%BcdEditExe%,Set,%BcdEditExe%,%Tools%\x86\bcdedit.exe
//source x64 host x86
If,Not,ExistFile,%BcdEditExe%,Message,"%Tools%\%OSArch%\bcdedit.exe NOT found. Multi-WIM Stop Now",Error
If,Not,ExistFile,%BcdEditExe%,Exit,"%Tools%\%OSArch%\bcdedit.exe NOT found. Multi-WIM Stop Now"
//-
If,Not,ExistDir,%OtherOS_Target%,DirMake,%OtherOS_Target%
//-
If,%BcdGrub_ScrollBox%,Equal,WinBCD-Menu,Run,%ScriptFile%,WinBCD_Menu
If,%BcdGrub_ScrollBox%,Equal,Grub4Dos-Menu,Run,%ScriptFile%,Grub4Dos_Menu
//-
StrFormat,CTRIM,%pFileBox5%,\,%Source_ProgPath%
If,%pCheckBox5%,Equal,True,run,%ScriptFile%,CopyPrograms,%Source_ProgPath%
StrFormat,CTRIM,%pFileBox6%,\,%Source_ProgPath%
If,%pCheckBox6%,Equal,True,run,%ScriptFile%,CopyProgramsCustom,%Source_ProgPath%
//-
StrFormat,CTRIM,%pFileBox7%,\,%Addboot3264Path%
If,%pCheckBox7%,Equal,True,run,%ScriptFile%,Addboot3264,%Addboot3264Path%
//-
FileDelete,%TargetDir%\boot\*.LOG*

[WinBCD_Menu]
// Windows Bootmanager after Grub4Dos
Set,%AddUEFI%,""
If,ExistFile,%TargetDir%\efi\microsoft\boot\bcd,Begin
Set,%BcdEFIexist%,Yes
If,%AddUEFI_CheckBox%,Equal,True,Set,%AddUEFI%," UEFI"
End
Else,Set,%BcdEFIexist%,No
//-
If,%pCheckBox2%,Equal,True,Run,%ScriptFile%,BCD_WIM,2
If,%pCheckBox3%,Equal,True,Run,%ScriptFile%,BCD_WIM,3
If,%pCheckBox4%,Equal,True,Run,%ScriptFile%,BCD_WIM,4
//-
Set,%tmout%,%Timeout_NumberBox%
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /set {default} description #$q%BcdStdDesc_TextBox%#$q",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /set {bootmgr} timeout %tmout%",%Tools%\%OSArch%\
If,%BcdEFIexist%,Equal,Yes,Begin
ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /set {default} description #$q%BcdStdDesc_TextBox%%AddUEFI%#$q",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /set {bootmgr} timeout %tmout%",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /set {memdiag} description #$qWindows Memory Diagnostic %SourceArch%%AddUEFI%#$q",%Tools%\%OSArch%\
End

[BCD_WIM]
If,Not,ExistFile,%pFileBox#1%,Run,%ScriptFile%,Warn_Message,"WimFile #1 selected does not exist. It will not be added"
Else,Begin
strformat,filename,%pFileBox#1%,%WIM#1_Filename%
Echo,"Copy %WIM#1_Filename%..."
FileCopy,%pFileBox#1%,%OtherOS_Target%
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /create %WIM#1_GUID% /d #$q%pTextBox#1%#$q /application osloader",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /set %WIM#1_GUID% locale %DistLang%",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /set %WIM#1_GUID% systemroot \Windows",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /set %WIM#1_GUID% detecthal Yes",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /set %WIM#1_GUID% winpe Yes",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /set %WIM#1_GUID% EMS Yes",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /set %WIM#1_GUID% Quietboot Yes",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /set %WIM#1_GUID% path \windows\system32\boot\winload.exe",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /set %WIM#1_GUID% osdevice ramdisk=[boot]\boot\img\%WIM#1_Filename%#$c{7619dcc8-fafe-11d9-b411-000476eba25f}",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /set %WIM#1_GUID% device ramdisk=[boot]\boot\img\%WIM#1_Filename%#$c{7619dcc8-fafe-11d9-b411-000476eba25f}",%Tools%\%OSArch%\
// Win8: Show progress bars "windows is loading". Win8 x64 (patch drivers signing) add: NoIntegrityChecks 1 / loadoptions DISABLE_INTEGRITY_CHECKS
If,%Wim#1_Win8x64_CheckBox%,Equal,True,Begin
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /set %WIM#1_GUID% bootmenupolicy legacy",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /set %WIM#1_GUID% NoIntegrityChecks Yes",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /set %WIM#1_GUID% loadoptions DISABLE_INTEGRITY_CHECKS",%Tools%\%OSArch%\
End
Else,Begin
If,%Wim#1_Win8_CheckBox%,Equal,True,ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /set %WIM#1_GUID% bootmenupolicy legacy",%Tools%\%OSArch%\
End
//-
If,%BcdEFIexist%%Wim#1_EFI_CheckBox%,Equal,YesTrue,Begin
ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /create %WIM#1_GUID% /d #$q%pTextBox#1%%AddUEFI%#$q /application osloader",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /set %WIM#1_GUID% locale %DistLang%",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /set %WIM#1_GUID% systemroot \Windows",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /set %WIM#1_GUID% detecthal Yes",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /set %WIM#1_GUID% winpe Yes",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /set %WIM#1_GUID% EMS Yes",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /set %WIM#1_GUID% Quietboot Yes",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /set %WIM#1_GUID% path \windows\system32\boot\winload.efi",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /set %WIM#1_GUID% osdevice ramdisk=[boot]\boot\img\%WIM#1_Filename%#$c{7619dcc8-fafe-11d9-b411-000476eba25f}",%Tools%\%OSArch%\
ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /set %WIM#1_GUID% device ramdisk=[boot]\boot\img\%WIM#1_Filename%#$c{7619dcc8-fafe-11d9-b411-000476eba25f}",%Tools%\%OSArch%\
// Win8 Win8 x64 : Show progress bars "windows is loading".
If,%Wim#1_Win8x64_CheckBox%,Equal,True,ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /set %WIM#1_GUID% bootmenupolicy legacy",%Tools%\%OSArch%\
If,%Wim#1_Win8_CheckBox%,Equal,True,ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /set %WIM#1_GUID% bootmenupolicy legacy",%Tools%\%OSArch%\
End
//
If,#1,Equal,2,Begin
ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /displayorder {default} %WIM2_GUID%",%Tools%\%OSArch%\
If,%BcdEFIexist%%Wim#1_EFI_CheckBox%,Equal,YesTrue,ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /displayorder {default} %WIM2_GUID%",%Tools%\%OSArch%\
End
If,#1,Equal,3,Begin
If,%pCheckBox2%,Equal,False,ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /displayorder {default} %WIM3_GUID%",%Tools%\%OSArch%\
If,%pCheckBox2%,Equal,True,ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /displayorder {default} %WIM2_GUID% %WIM3_GUID%",%Tools%\%OSArch%\
If,%BcdEFIexist%%Wim#1_EFI_CheckBox%,Equal,YesTrue,Begin
If,%pCheckBox2%,Equal,False,ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /displayorder {default} %WIM3_GUID%",%Tools%\%OSArch%\
If,%pCheckBox2%,Equal,True,ShellExecute,Hide,%BcdEditExe%,"/store %BCDefiStore% /displayorder {default} %WIM2_GUID% %WIM3_GUID%",%Tools%\%OSArch%\
End
End
If,#1,Equal,4,Begin
If,%pCheckBox2%,Equal,False,If,%pCheckBox3%,Equal,False,ShellExecute,Hide,%BcdEditExe%,"/store %BCDStore% /displayorder {default} %WIM4_GUID%",%Tools%\%OSArch%\
